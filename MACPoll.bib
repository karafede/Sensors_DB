@article{kim_microfabricated_2012-1,
	title = {Microfabricated {Gas} {Chromatograph} for {On}-{Site} {Determinations} of {TCE} in {Indoor} {Air} {Arising} from {Vapor} {Intrusion}. 2. {Spatial}/{Temporal} {Monitoring}},
	volume = {46},
	issn = {0013-936X},
	url = {http://dx.doi.org/10.1021/es204625w},
	doi = {10.1021/es204625w},
	abstract = {We demonstrate the use of two prototype Si-microfabricated gas chromatographs (?GC) for continuous, short-term measurements of indoor trichloroethylene (TCE) vapor concentrations related to the investigation of TCE vapor intrusion (VI) in two houses. In the first house, with documented TCE VI, temporal variations in TCE air concentrations were monitored continuously for up to 48 h near the primary VI entry location under different levels of induced differential pressure (relative to the subslab). Concentrations ranged from 0.23 to 27 ppb by volume (1.2?150 ?g/m3), and concentration trends agreed closely with those determined from concurrent reference samples. The sensitivity and temporal resolution of the measurements were sufficiently high to detect transient fluctuations in concentration resulting from short-term changes in variables affecting the extent of VI. Spatial monitoring showed a decreasing TCE concentration gradient with increasing distance from the primary VI entry location. In the second house, with no TCE VI, spatial profiles derived from the ?GC prototype data revealed an intentionally hidden source of TCE within a closet, demonstrating the capability for locating non-VI sources. Concentrations measured in this house ranged from 0.51 to 56 ppb (2.7?300 ?g/m3), in good agreement with reference method values. This first field demonstration of ?GC technology for automated, near-real-time, selective VOC monitoring at low- or subppb levels augurs well for its use in short- and long-term on-site analysis of indoor air in support of VI assessments.},
	number = {11},
	urldate = {2014-06-13},
	journal = {Environmental Science \& Technology},
	author = {Kim, Sun Kyu and Burris, David R. and Bryant-Genevier, Jonathan and Gorder, Kyle A. and Dettenmaier, Erik M. and Zellers, Edward T.},
	month = jun,
	year = {2012},
	note = {00009},
	pages = {6073--6080},
	file = {ACS Full Text Snapshot:C\:\\Users\\karaf\\Zotero\\storage\\2ZZDXST9\\es204625w.html:text/html;Kim et al. - 2012 - Microfabricated Gas Chromatograph for On-Site Dete.pdf:C\:\\Users\\karaf\\Zotero\\storage\\EH6G3962\\Kim et al. - 2012 - Microfabricated Gas Chromatograph for On-Site Dete.pdf:application/pdf}
}

