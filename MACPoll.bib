	keywords = {Electronic nose, Sensor array, Nonexponential relaxations, Stretched exponential low},
	pages = {101--113},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\karaf\\Zotero\\storage\\N9GA4BRX\\Snopok and Kruglenko - 2005 - Nonexponential relaxations in sensor arrays forec.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\karaf\\Zotero\\storage\\RU27HGP2\\S092540050400406X.html:text/html}
}

