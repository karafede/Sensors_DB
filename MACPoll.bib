	author = {Chaiwatpongsakorn, C. and Lu, M. and Keener, T.C. and Khang, S.-J.},
	year = {2014},
	keywords = {Monitoring, Carbon monoxide, Intersection, Traffic, Wireless sensor network},
	pages = {6246--6264},
	file = {Chaiwatpongsakorn et al. - 2014 - The deployment of carbon monoxide wireless sensor .html:C\:\\Users\\karaf\\Zotero\\storage\\TVJJ64H7\\Chaiwatpongsakorn et al. - 2014 - The deployment of carbon monoxide wireless sensor .html:text/html;SCOPUS Snapshot:C\:\\Users\\karaf\\Zotero\\storage\\RAR72E8C\\display.html:text/html}
