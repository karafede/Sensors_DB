	month = mar,
	year = {2007},
	keywords = {Ozone, Kinetics, SnO2, sensor, Humidity influence, Thin films},
	pages = {519--526},
	file = {Korotcenkov et al. - 2007 - Effect of air humidity on gas response of SnO2 thi.pdf:C\:\\Users\\karaf\\Zotero\\storage\\7JDDGC9R\\Korotcenkov et al. - 2007 - Effect of air humidity on gas response of SnO2 thi.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\karaf\\Zotero\\storage\\2R4VVQSV\\S092540050600459X.html:text/html}
}

@article{korotcenkov_kinetics_2007,
