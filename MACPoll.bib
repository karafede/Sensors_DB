	pages = {889--893},
	file = {Koley et al. - 2010 - Highly sensitive and selective NO2 sensing using e.pdf:C\:\\Users\\karaf\\Zotero\\storage\\BPACKVBP\\Koley et al. - 2010 - Highly sensitive and selective NO2 sensing using e.pdf:application/pdf;Koley et al. - 2010 - Highly sensitive and selective NO2 sensing using e.pdf:C\:\\Users\\karaf\\Zotero\\storage\\DPXGBZME\\Koley et al. - 2010 - Highly sensitive and selective NO2 sensing using e.pdf:application/pdf}
