	pages = {1064--1069 Vol.2},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\karaf\\Zotero\\storage\\UWR6J4BZ\\abs_all.html:text/html;Kammerer et al_2003_An intelligent fuel sensor based on a microstructured gas sensor.pdf:C\:\\Users\\karaf\\Zotero\\storage\\K2VSIKH3\\Kammerer et al_2003_An intelligent fuel sensor based on a microstructured gas sensor.pdf:application/pdf}
