	journal = {Atmospheric Environment},
	author = {Chen, Kai and Yang, Hai Bing and Ma, Zong Wei and Bi, Jun and Huang, Lei},
	month = nov,
	year = {2013},
	keywords = {Ozone, Temperature, Morality},
	pages = {119--128},
