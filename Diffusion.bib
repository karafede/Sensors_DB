@misc{spinelle_calibration_2014,
	address = {Delft, The Netherlands},
	type = {Poster presentation},
	title = {Calibration of a cluster of low-cost sensors for the measurement of air pollution in ambient air},
	abstract = {The objective of this task is to reduce the uncertainty of single sensor based measurements using a cluster system able to measure a broad range of pollutants. Based on the results of evaluation and validation of low-costs sensors carried out during the first part of the project, several sensors have been grouped in a clustered system able to detect NO/NO2/NOx, CO, SO2 and O3 which are pollutants regulated in 2008/50/EEC). To extend the range of species, CO2 sensors and PM2.5 and PM10 personal aerosol monitor have been added.

The cluster consists of 3 NO2 sensors and 2 CO sensors, both electrochemical and metal oxide type, 1 NO and 2 O3 electrochemical sensors, 2 infrared CO2 sensors and 1 PM2.5 and 1 PM10 laser photometers. Experiment is carried out with the collaboration of the ERLAP Laboratory at an air conditioned mobile laboratory equipped with meteorological sensors and reference gas analyzers. These reference measurements are used for data validation, comparison and data treatment of sensor responses. 

Based on the outcome of the procedure of evaluation and calibration  and the single reports of validation, both performances and interfering parameters of such a cluster are investigated using deterministic or statistical methods such as discriminant analysis and neural networks.},
	author = {Spinelle, L. and Villani, M. G. and Suriano, D. and Penza, M. and Gerboles, M. and Aleixandre, M.},
