	urldate = {2011-04-26},
	journal = {Talanta},
	author = {Canepari, S. and Astolfi, M.L. and Moretti, S. and Curini, R.},
	month = jul,
	year = {2010},
	keywords = {Airborne particulate matter, Efficiency of extracting solutions, Elemental solubility, Selectivity of source tracers, Size-segregated samples},
	pages = {834--844},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\karagfe\\Zotero\\storage\\G7GVBTHS\\Canepari et al. - 2010 - Comparison of extracting solutions for elemental f.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\karagfe\\Zotero\\storage\\H7657DVT\\science.html:text/html}
