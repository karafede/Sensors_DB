@article{wolff_new_2011,
	title = {A new fundamental parameter based calibration procedure for micro {X}-ray fluorescence spectrometers},
	volume = {66},
	issn = {0584-8547},
	url = {http://www.sciencedirect.com/science/article/B6THN-5236R8V-1/2/861f276f4935ae7b08c6014dcf450c97},
	doi = {10.1016/j.sab.2011.01.009},
	abstract = {Fundamental parameter based quantification of X-ray fluorescence (XRF) measurement data requires an accurate knowledge of the spectrometer parameters, including the spectral distribution of the excitation radiation. In case of micro-XRF where a polycapillary optic is utilized in the excitation channel this distribution is changed due to the transmission properties of the lens. A new calibration procedure, based on fluorescence data of thin standard samples, was developed to determine the excitation spectrum, i.e., the product of the X-ray tube spectrum and the transmission of the used X-ray optic of a micro-XRF setup. The calibration result was validated by the quantitative analyses of certified multi-element reference standards and shows uncertainties in the order of 2\% for main components, 10\% for minor elements and 25\% for trace elements. The influence of secondary order effects like Coster-Kronig transitions and cascade effects is analyzed and the accuracy of fundamental parameters in common databases is discussed.},
	number = {2},
	urldate = {2011-03-14},
	journal = {Spectrochimica Acta Part B: Atomic Spectroscopy},
	author = {Wolff, Timo and Malzer, Wolfgang and Mantouvalou, Ioanna and Hahn, Oliver and KanngieÃer, Birgit},
	month = feb,
	year = {2011},
	keywords = {Micro-XRF calibration, Polycapillary optic, Reference free quantification, Transmission},
	pages = {170--178},
	file = {A new fundamental parameter based calibration procedure for micro X-ray.pdf:C\:\\Users\\karagfe\\Zotero\\storage\\T9Q2NIQA\\A new fundamental parameter based calibration procedure for micro X-ray.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\karagfe\\Zotero\\storage\\39P6SA6W\\science.html:text/html}
}

@article{molnar_domestic_2005,
