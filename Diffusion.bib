@article{miller_characterization_2005,
	title = {Characterization of small particles by micro {X}-ray fluorescence},
	volume = {60},
	issn = {0584-8547},
	url = {http://www.sciencedirect.com/science/article/B6THN-4HDG9FT-2/2/8493ce8b926d4dcbe06698c477e1817d},
	doi = {10.1016/j.sab.2005.09.003},
	abstract = {{\textless}p{\textgreater}{\textless}br/{\textgreater}Micro X-ray fluorescence was used to study both homogeneous and heterogeneous particle systems. Specifically, homogeneous glass microspheres and heterogeneous soil particle samples were prepared by both bulk and single particle sample preparation methods for evaluation by micro X-ray fluorescence. Single particle sample preparation methods allow for single particles from a collected sample to be isolated and individually presented to the micro X-ray fluorescence instrument for analysis. Various particle dispersion methods, including immobilization onto Tacky Dot(TM) slides, mounting onto double-sided sticky tape affixed to polypropylene film, or attachment to polypropylene film using 3M Artist's Adhesive, were used to separate the sample particles for single particle analysis. These methods were then compared and evaluated for their ability to disperse the particles into an array of single separated particles for optimal micro X-ray fluorescence characterization with minimal background contribution from the particle mounting surface. Bulk methods of particle sample preparation, which included pellet preparation and aerosol impaction, used a large quantity of collected single particles to make a single homogeneous specimen for presentation to the instrument for analysis. It was found that single particle elemental analysis by micro X-ray fluorescence can be performed if the particles are well separated (minimum separation distanceÂ =Â excitation source beam diameter) down to a particle mass of {\textasciitilde}Â 0.04 ng and a mean particle diameter of {\textasciitilde}Â 0.06 [mu]m. Homogeneous particulates can be adequately characterized by micro X-ray fluorescence using either bulk or single particle analysis methods, with no loss of analytical information. Heterogeneous samples are much harder to characterize, and both single particle as well as bulk analyses must be performed on the sample to insure full elemental characterization by micro X-ray fluorescence.{\textless}/p{\textgreater}},
	number = {11},
	urldate = {2011-04-21},
	journal = {Spectrochimica Acta Part B: Atomic Spectroscopy},
	author = {Miller, Thomasin C. and Langley DeWitt, Helen and Havrilla, George J.},
	month = nov,
	year = {2005},
	keywords = {Aerosol, Soil, Cascade impactor, Glass microsphere, Micro X-ray fluorescence (MXRF), Particle analysis},
	pages = {1458--1467},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\karagfe\\Zotero\\storage\\C2XSM98M\\Miller et al. - 2005 - Characterization of small particles by micro X-ray.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\karagfe\\Zotero\\storage\\FSMIWUX4\\science.html:text/html}
}

