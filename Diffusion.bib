	author = {Manalis, N. and Grivas, G. and Protonotarios, V. and Moutsatsou, A. and Samara, C. and Chaloulakou, A.},
	month = jul,
	year = {2005},
	keywords = {Principal component analysis, Road traffic, Emission sources, Trace metals},
	pages = {557--566},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\karagfe\\Zotero\\storage\\GJET3U6R\\Manalis et al. - 2005 - Toxic metal content of particulate matter (PM10), .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\karagfe\\Zotero\\storage\\7448798C\\science.html:text/html}
